/// FCA Version Table
table audit_latest_available_fca_version
	lineageTag: 73ef1116-064e-4550-8c98-f45b50ca8628
	sourceLineageTag: [dbo].[audit_latest_available_fca_version]

	/// Indicates whether an FCA update is available by comparing the current FCA version with the latest available version.
	measure 'FCA Update Available' = ```
			
						IF([FCA Release] == MAX(audit_latest_available_fca_version[current_fca_version]), 0, 1)
			```
		formatString: 0
		lineageTag: 8f8ddc82-e24d-493e-a6b3-203142979dfa

		changedProperty = Name

		changedProperty = Description

	/// A static string value for FCA Release.
	measure 'FCA Release' = "2025.9.2"
		lineageTag: ba9a1bef-cb0e-42f4-8d14-ad6467ada7b7

		changedProperty = Name

		changedProperty = Description

	/// Displays a string message based on whether the FCA Release is equal to the maximum current FCA version.
	measure 'FCA Release Message' = ```
			
						IF(
						    [FCA Release]== MAX(audit_latest_available_fca_version[current_fca_version]),
						     "FCA is up-to-date",
						     "Update is available"
						    )
			```
		lineageTag: 0fc23fce-bf93-42a8-bb4c-84a204691719

